{
  "name": "anime-suupu",
  "version": "0.8.16",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --env development --hot --host 0.0.0.0 --port 8010",
    "start:ci": "NODE_ENV=ci webpack-dev-server --env ci --port 8011",
    "build": "NODE_ENV=production webpack --config webpack.config.babel.ts --env production",
    "lint": "eslint 'src/**/*' -c .eslintrc --fix && prettier --write 'src/**/*'",
    "test:ci": "npm run test:unit && npm run test:integration:ci:run",
    "test:unit": "jest",
    "test:unit:watch": "jest --watchAll",
    "cypress:open": "cypress open",
    "test:integration": "cypress run --config-file cypress/dev.config.json",
    "test:integration:headless": "cypress run --headless --config-file cypress/dev.config.json",
    "test:integration:ci": "cypress run --headless --config-file cypress/ci.config.json",
    "test:integration:ci:run": "start-server-and-test start:ci http://localhost:8011 test:integration:ci",
    "stats:prod": "webpack --config webpack.config.babel.ts --env production --progress --profile --json > prod.stats.json",
    "stats:dev": "webpack --config webpack.config.babel.ts --env dev --progress --profile --json > dev.stats.json",
    "release": "standard-version",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint --color --edit -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/*": [
      "eslint -c .eslintrc --fix",
      "prettier --write"
    ]
  },
  "keywords": [],
  "author": "Sergio Rodrigo Royo",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@babel/register": "^7.10.5",
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^12.1.4",
    "@hot-loader/react-dom": "^16.13.0",
    "@types/enzyme": "^3.10.8",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.9",
    "@types/react": "^16.9.44",
    "@types/react-dom": "^16.9.8",
    "@types/react-hot-loader": "^4.1.1",
    "@types/webpack-bundle-analyzer": "^3.8.0",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.2.2",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.0.3",
    "cypress": "^4.12.1",
    "eslint": "^7.28.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-airbnb-typescript": "^9.0.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsdoc": "^35.2.0",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^4.0.8",
    "eslint-plugin-unicorn": "^21.0.0",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^6.0.0",
    "jest": "^26.2.2",
    "lint-staged": "^10.2.11",
    "prettier": "^2.3.1",
    "standard-version": "^8.0.2",
    "start-server-and-test": "^1.11.2",
    "ts-jest": "^26.1.4",
    "ts-loader": "^8.0.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.9",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.13.0",
    "webpack-merge": "^5.1.1"
  }
}
